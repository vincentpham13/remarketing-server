{
  "name": "typescript-express-starter",
  "version": "1.0.1",
  "description": "",
  "main": "main.ts",
  "scripts": {
    "dev": "NODE_ENV=development tsnd --poll --interval 600 --no-notify --watch src --ignore-watch src/migrations --ignore-watch '__tests__/*.spec.ts' --ignore-watch node_modules -r tsconfig-paths/register ./src/main.ts",
    "dev2": "NODE_ENV=development nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' -r tsconfig-paths/register ./src/main.ts",
    "clean": "shx rm -rf build/*",
    "copy": "cpx 'knexfile.ts' 'build/' && cpx 'src/migrations/**' 'build/src/migrations' && cpx 'tsconfig.json' 'build/'",
    "lint": "eslint \"src/**/*.ts\" --ignore-pattern '__tests__/*.spec.ts'",
    "prebuild": "npm run lint && npm run clean",
    "build:dev": "npm run prebuild && tsc && shx rm -rf build/src/migrations && npm run copy",
    "build:staging": "npm run prebuild && tsc && npm run copy",
    "start:dev": "TS_NODE_FILES=true && TS_NODE_PROJECT='build/tsconfig.json' && node -r ts-node/register -r tsconfig-paths/register build/src/main.js",
    "start:staging": "NODE_ENV=production node -r tsconfig-paths/register src/main.js",
    "migration": "./node_modules/.bin/knex migrate:latest",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/express-useragent": "^1.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.22",
    "@types/node-fetch": "^2.5.8",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cpx": "^1.5.0",
    "eslint": "^6.5.1",
    "express": "^4.17.1",
    "express-useragent": "^1.0.15",
    "inversify": "^5.0.5",
    "inversify-express-utils": "^6.3.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.15",
    "knex-logger": "^0.1.0",
    "knex-stringcase": "^1.4.3",
    "moment-timezone": "^0.5.32",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "shx": "^0.3.3",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "ts-node-dev": "^1.1.1"
  }
}
